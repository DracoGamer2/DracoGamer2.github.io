{
  "version": 2,
  "builds": [
    {
      "src": "api-state.js",
      "use": "@vercel/node"
    },
    {
      "src": "api-update.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api-state",
      "dest": "/api-state.js"
    },
    {
      "src": "/api-update",
      "dest": "/api-update.js"
    }
  ]
}
